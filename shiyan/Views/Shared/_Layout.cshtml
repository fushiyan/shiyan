 <!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title - Hello Quebec Center</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <div class="top-bg">
        <div class="bottom-bg">
            <div class="main-cont">

                <div class="header">
                    <div class="logo-block">
                        <h1><a href="#">Hello Quebec Center</a></h1>
                    </div>
                    <div class="menu">

                        <ul class="main-menu">
                            <li class="selected"><a href="#" class="home-icon"><span class="left"><span class="right"><span class="icon">Home</span></span></span></a></li>
                            <li><a href="#" class="messages-icon"><span class="icon">Messages<span class="notification">3</span></span></a></li>
                            <li>
                                <a href="#" class="users-icon"><span class="icon"><span class="arrow">Accounts</span></span></a>
                                <ul class="submenu">
                                    <li><a href="#">Learners</a></li>
                                    <li><a href="#">Tutors</a></li>
                                    <li><a href="#">Clients</a></li>
                                    <li><a href="#">Classrooms</a></li>
                                </ul>
                            </li>
                            <li><a href="#" class="requests-icon"><span class="icon">Requests<span class="notification">2</span></span></a></li>
                            <li><a href="#" class="reports-icon"><span class="icon">Reports</span></a></li>
                            <li><a href="#" class="settings-icon"><span class="icon">Settings</span></a></li>
                            <li><a href="#" class="feedback-icon"><span class="icon">Feedback</span></a></li>
                            <li><a href="#" class="forum-icon"><span class="icon">Forum</span></a></li>
                        </ul>
                    </div>
                    <div class="right-block">
                        <div class="inner">
                            @Html.Partial("_LoginPartial")
                        </div>
                    </div>
                </div>
                @RenderBody()

                <div class="footer">
                    <div class="left">
                        &copy; 2016 Hello Quebec Center
                    </div>
                </div>
            </div>
        </div>
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>


   
 